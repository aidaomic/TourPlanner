<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TourPlanner.Controller">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="500" minWidth="10.0" prefWidth="400.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="400" minWidth="10.0" prefWidth="300.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="195.0" minHeight="10.0" prefHeight="50" valignment="CENTER" vgrow="NEVER" />
        <RowConstraints maxHeight="321.0" minHeight="10.0" prefHeight="50" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="321.0" minHeight="10.0" prefHeight="300" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <TextField fx:id="InputTextField" text="Search for Tour by number" GridPane.columnIndex="0" GridPane.rowIndex="1">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </TextField>
        <Button mnemonicParsing="false" onAction="#calculateOutput" prefWidth="249.0" text="Search" GridPane.columnIndex="1" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin>
        </Button>
        <Label fx:id="OutputLabel" style="-fx-alignment: center;" textAlignment="CENTER" GridPane.rowIndex="2">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" text="File" onAction="#getFile"/>
          <Button mnemonicParsing="false" text="Edit" onAction="#doEdit"/>
          <Button mnemonicParsing="false" text="Options" onAction="#getOptions"/>
        </buttons>
      </ButtonBar>
        <Button mnemonicParsing="false" onAction="#getHelp" prefWidth="249.0" text="Help" GridPane.columnIndex="1" GridPane.hgrow="SOMETIMES">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin>
        </Button>
    </children>
</GridPane>
